  body
    section.ftco-section.ftco-no-pt.ftco-no-pb.contact-section
      .container-fluid.px-0
        .row.no-gutters.block-9
          .col-md-6.order-md-last.d-flex
            form.bg-light.p-5.contact-form(action='/register', method="POST")
              .form-group
                input.form-control(type='text', name='name', placeholder='Nombre')
              .form-group
                input.form-control(type='text', name='email', placeholder='goku98@correo.com')
              .form-group
                input.form-control(type='password', name='password', placeholder='*****')
              .form-group
                input.form-control(type='password', name='password2', placeholder='*****')
              .form-group
                input.btn.btn-primary.py-3.px-5(type='submit', value='Submit')


          script(src='js/jquery.min.js')
          script(src='js/jquery-migrate-3.0.1.min.js')
          script(src='js/popper.min.js')
          script(src='js/bootstrap.min.js')
          script(src='js/jquery.easing.1.3.js')
          script(src='js/jquery.waypoints.min.js')
          script(src='js/jquery.stellar.min.js')
          script(src='js/owl.carousel.min.js')
          script(src='js/jquery.magnific-popup.min.js')
          script(src='js/aos.js')
          script(src='js/jquery.animateNumber.min.js')
          script(src='js/scrollax.min.js')
          script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false')
          script(src='js/google-map.js')
          script(src='js/main.js')
